{
  "name": "brace-client-web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@blockstack/connect": "^2.7.2",
    "@fullhuman/postcss-purgecss": "^2.2.0",
    "@redux-offline/redux-offline": "https://github.com/bracedotto/redux-offline#88a89e7ebe76415e83bd1943360e817274d8063d",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "blockstack": "^21.0.0",
    "jdenticon": "^2.2.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-graceful-image": "^1.5.0",
    "react-redux": "^7.2.0",
    "react-scripts": "3.4.1",
    "react-stack-grid": "^0.7.1",
    "redux": "^4.0.5",
    "redux-loop": "^5.1.1",
    "reselect": "^4.0.0",
    "source-map-explorer": "^2.4.2",
    "tailwindcss": "^1.4.6",
    "url-parse": "^1.4.7"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start-css": "postcss tailwind.css -o src/stylesheets/tailwind.css --watch",
    "build-css": "postcss tailwind.css -o src/stylesheets/tailwind.css --env production",
    "build-css-dev": "postcss tailwind.css -o src/stylesheets/tailwind.css",
    "start-js": "BROWSER=none react-scripts start",
    "build-js": "react-scripts build",
    "start": "npm-run-all -p start-css start-js",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
